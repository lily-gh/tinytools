#default word color
#regexp=[\w.,\:\-_/]+
regexp=.+
colours="\033[38;5;39m"
-

#table borders
regexp=[+\-]+[+\-]|[|]
colours="\033[38;5;213m"
-

#data in ( ) and ' '
regexp=\([\w\d,']+\)
colours=white
-

#numeric
regexp=\s[\d\.]+\s*($|(?=\|))
colours=bright_yellow
-

#date
regexp=\d{4}-\d{2}-\d{2}
colours="\033[38;5;41m"
-
#time
regexp=\d{2}:\d{2}:\d{2}
colours="\033[38;5;41m"
-

#IP
regexp=(\d{1,3}\.){3}\d{1,3}(:\d{1,5})?
colours="\033[38;5;87m"
-

#schema
regexp=`\w+`
colours=bright_yellow
-

#email
regexp=[\w\.\-_]+@[\w\.\-_]+
colours="\033[38;5;99m"
-

#row delimeter when using \G key
regexp=[*]+.+[*]+
count=stop
colours="\033[38;5;215m"
-

#column names when using \G key
regexp=^\s*\w+:
colours=white
-

#NULL values
regexp=\bNULL\b
colours=bright_red
-